{
  "env": {
    "development": {
      "presets": ["@babel/env", "@babel/react", "@babel/preset-typescript"],
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "fileName": false,
            "pure": true
          }
        ]
      ]
    },
    "production": {
      "presets": ["@babel/env", "@babel/react", "@babel/preset-typescript"],
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "fileName": false,
            "pure": true
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}
