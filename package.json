{
  "name": "fleet-icons",
  "version": "0.1.0",
  "author": "daacdev",
  "description": "Bootstrap Icons for Styled Components",
  "homepage": "https://daacdev.github.io/fleet-icons/",
  "license": "MIT",
  "module": "dist/fleet-icons.esm.js",
  "main": "dist/fleet-icons.cjs.js",
  "typings": "dist/fleet-icons.cjs.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/daacdev/fleet-icons.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "styled-components",
    "bootstrap-icons",
    "icons",
    "fleet",
    "styled-fleet",
    "fleet-icons"
  ],
  "files": [
    "dist",
    "icon"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "postinstall": "yarn generate && preconstruct dev",
    "prepare": "preconstruct build && is-ci || husky install",
    "build": "preconstruct build",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,json}\"",
    "typecheck": "tsc --noEmit",
    "commit": "git-cz",
    "generate": "rimraf src/icon && rimraf src/index.ts && rimraf icon && node scripts/generate",
    "clean:project": "rimraf dist && rimraf icon && rimraf src/icon && rimraf src/index.ts"
  },
  "peerDependencies": {
    "react": ">=16",
    "styled-components": ">=5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@commitlint/prompt-cli": "^12.1.1",
    "@preconstruct/cli": "^2.0.7",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-styled-components": "^1.12.0",
    "bootstrap-icons": "^1.4.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.3",
    "ts-jest": "^26.5.4",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "icon/**/index.tsx"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
